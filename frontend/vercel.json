{
    "version": 2,
    "framework": "vite",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "outputDirectory": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "src": "/(.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.svg|.*\\.ico|.*\\.woff|.*\\.woff2|.*\\.ttf|.*\\.eot)",
        "dest": "/$1",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "installCommand": "pnpm install"
  }
  