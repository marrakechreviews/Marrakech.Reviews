{
    "version": 2,
    "framework": "vite",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "outputDirectory": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  