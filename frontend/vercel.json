{
    "version": 2,
    "framework": "vite",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "outputDirectory": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "installCommand": "pnpm install"
  }
  